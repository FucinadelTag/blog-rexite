<!-- Nav Bar -->
<div class="contain-to-grid menu">
    <nav class="top-bar" data-topbar role="navigation">
        <ul class="title-area">
            <li class="name">
                <!--http://lh6.googleusercontent.com/-34YbdpE2f6c/U0025W216cI/AAAAAAAAlzE/-OIpzXG15X4/w160-h60-no/logo.png -->
                <a href="/"><img alt="Rexite" src="http://www.rexite.it/img/logo.png"></a>
            </li>
            <li class="toggle-topbar menu-icon"><a href="#"><span>Menu</span></a></li>
        </ul>
        <section class="top-bar-section">
            <!-- Right Nav Section -->
            <ul class="right">
                <li><%#= link_to 'Defibrillatori Philips', '/' %></li>
                <li><%= link_to 'Online Store', 'http://eu.rexitestore.com/' %></li>
                <li><%= link_to 'Corporate', 'http://www.rexite.it/' %></li>
            </ul>
        </section>
    </nav>
</div>


<%= yield %>

<footer class="row">
    <hr>
        <div class="row">
            <div class="large-8 medium-8 small-8 columns">
                <i>
                    Corporate site: <%= link_to 'http://www.rexite.it', 'http://www.rexite.it' %><br />
                    Ecommerce EU: <%= link_to 'http://eu.rexitestore.com/', 'http://eu.rexitestore.com/' %><br />
                    Ecommerce US: <%= link_to 'http://us.rexitestore.com/', 'http://us.rexitestore.com/' %><br />
                    Ecommerce IT: <%= link_to 'http://it.rexitestore.com/', 'http://it.rexitestore.com/' %>
                </i>
            </div>

            <div class="large-4 medium-4 small-4 columns">
                <%= link_to '<img alt="Rexite" src="http://www.rexite.it/img/logo.png">', '/' %></li>
                <br />
                <i>
                REXITE Srl - Via Edison 7<br />
                20090 Cusago (Milano) - Italy<br />
                P.IVA: 10574970017</i><br />


            </div>

        </div>
</footer>



<% if current_page.data.conversionCode  %>
    <%= current_page.data.conversionCode %>
<% end %>


<%= javascript_include_tag  "all" %>

<% if current_page.data.javacriptCode  %>
    <%= current_page.data.javacriptCode %>
<% end %>

<script type="text/javascript">
    $(document).ready(function() {
        var manageCampainObj = new manageCampain();
        manageCampainObj.doManage ();
    });
</script>



</body>


</html>